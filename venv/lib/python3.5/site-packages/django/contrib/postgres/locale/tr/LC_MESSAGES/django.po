#: venv/lib/python3.5/site-packages/django/contrib/postgres/apps.py:13
msgid "PostgreSQL extensions"
msgstr "PostgreSQL uzantıları"

#: venv/lib/python3.5/site-packages/django/contrib/postgres/fields/array.py:21
#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/array.py:16
#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/array.py:160
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "Dizilimdeki %(nth)s öğesi doğrulanmadı: "

#: venv/lib/python3.5/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "İç içe dizilimler aynı uzunlukta olmak zorunda."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/fields/hstore.py:14
msgid "Map of strings to strings/nulls"
msgstr "Dizgiler/boşlar olarak dizgilerin eşlemesi"

#: venv/lib/python3.5/site-packages/django/contrib/postgres/fields/hstore.py:16
#, python-format
msgid "The value of \"%(key)s\" is not a string or null."
msgstr "\"%(key)s\" değeri bir dizgi ya da boş değil."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/fields/jsonb.py:30
msgid "A JSON object"
msgstr "JSON nesnesi"

#: venv/lib/python3.5/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "Value must be valid JSON."
msgstr "Değer geçerli JSON olmak zorundadır."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "JSON verisi yüklenemedi."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Bir JSON dizini girilmek zorundadır."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/jsonb.py:19
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "'%(value)s' değeri geçerli JSON olmak zorundadır."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:16
msgid "Enter two valid values."
msgstr "Iki geçerli değer girin."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:17
msgid "The start of the range must not exceed the end of the range."
msgstr "Aralığın başlangıcı aralığın bitişini aşmamak zorundadır."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:64
msgid "Enter two whole numbers."
msgstr "Bütün iki sayıyı girin."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:70
msgid "Enter two numbers."
msgstr "İki sayı girin."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:76
msgid "Enter two valid date/times."
msgstr "Geçerli iki tarih/saat girin."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:82
msgid "Enter two valid dates."
msgstr "Geçerli iki tarih girin."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:14
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha fazla "
"içermemelidir."
msgstr[1] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha fazla "
"içermemelidir."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:21
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha az "
"içermemelidir."
msgstr[1] ""
"Liste %(show_value)d öğe içeriyor, %(limit_value)d değerden daha az "
"içermemelidir."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:31
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Bazı anahtarlar eksik: %(keys)s"

#: venv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:32
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Bazı bilinmeyen anahtarlar verilmiş: %(keys)s"

#: venv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:73
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Bu aralığın %(limit_value)s değerinden küçük veya eşit olduğundan emin olun."

#: venv/lib/python3.5/site-packages/django/contrib/postgres/validators.py:79
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Bu aralığın %(limit_value)s değerinden büyük veya eşit olduğundan emin olun."
